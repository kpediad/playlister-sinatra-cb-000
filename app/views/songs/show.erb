<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>
<h1><%=@song.name%></h1>
<h2><a href="/artists/<%=@song.artist.slug%>"><%=@song.artist.name%></a></h2>
<h2>Genres</h2>
<ul>
  <%@song.genres.each do |genre|%>
    <li><a href="/genres/<%=genre.slug%>"><%=genre.name%></a></li>
  <%end%>
</ul>
<br>
<form action="/songs/<%=@song.slug%>" method="get">
  <input type="submit" name="submit" value="Edit Song">
</form>
